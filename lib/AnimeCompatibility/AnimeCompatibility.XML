            </summary>
            <param name="user"></param>
            <param name="animeList">Null if the user has no anime list (which is different from not being cached).</param>
            <returns>True if the user was in the cache, false if not.</returns>
        </member>
        <member name="M:AnimeCompatibility.IMyAnimeListCache.PutListForUser(System.String,System.Collections.Generic.ICollection{AnimeCompatibility.MyAnimeListEntry})">
